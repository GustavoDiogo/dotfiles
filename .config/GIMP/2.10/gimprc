# GIMP gimprc
# 
# This is your personal gimprc file.  Any variable defined in this file takes
# precedence over the value defined in the system-wide gimprc:
# /etc/gimp/2.0/gimprc
# Most values can be set within GIMP by changing some options in the
# Preferences dialog.

(default-image-dockable-point "colormap-editor")
(default-image-dockable-point-2 "device-status")
(default-image-dockable-point-3 "undo-history")
(theme "Dark")
(icon-theme "Symbolic")
(font-relative-size 1.000000)
(toolbox-color-area yes)
(toolbox-foo-area no)
(toolbox-image-area no)
(toolbox-wilber yes)
(toolbox-groups yes)
(theme-path "${gimp_dir}/themes:${gimp_data_dir}/themes")
(icon-theme-path "${gimp_dir}/icons:${gimp_data_dir}/icons")
(undo-levels 5)
(undo-size 1073741824)
(transparency-type GRAY_CHECKS)
(transparency-size LARGE_CHECKS)
(snap-distance 8)
(marching-ants-speed 200)
(resize-windows-on-zoom no)
(resize-windows-on-resize no)
(default-dot-for-dot yes)
(initial-zoom-to-fit yes)
(cursor-mode tool-crosshair)
(cursor-updating yes)
(show-brush-outline yes)
(show-paint-tool-cursor yes)
(image-title-format "%D*%f-%p.%i (%t, %o, %L) %wx%h")
(image-status-format "%n (%m)")
(monitor-xresolution 96.000000)
(monitor-yresolution 96.000000)
(save-device-status yes)
(devices-share-tool no)
(save-session-info yes)
(restore-session yes)
(save-tool-options yes)
(show-tooltips yes)
(tearoff-menus yes)
(can-change-accels no)
(save-accels yes)
(restore-accels yes)
(last-opened-size 10)
(max-new-image-size 134217728)
(toolbox-window-hint normal)
(dock-window-hint normal)
(keep-toolbox-on-top no)
(toolbox-group-menu-mode show-on-hover)
(layer-boundary-style-size 1)
(layer-previews yes)
(layer-preview-size medium)
(thumbnail-size normal)
(thumbnail-filesize-limit 4194304)
(thumbnail-layer-boundary-size-limit 1024)
(install-colormap no)
(min-colors 144)

# end of gimprc
